{
  "compilerOptions": {
    "experimentalDecorators": true,   
    "outDir": "./dist",
    "rootDir": "src",
    "target": "es2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  }
}
